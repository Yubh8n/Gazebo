<!-- launch file -->
<launch>
	<arg name="heartbeats" default="false" />
	<node pkg="mavlink_lora" type="mavlink_lora_node" name="mavlink_lora" respawn="true" output="screen">
		<param name="serial_device" value="/tmp/ttyV6"/>
		<param name="serial_baudrate" value="921600"/>
		<param name="heartbeats" value="$(arg heartbeats)" />
	</node>
</launch>

